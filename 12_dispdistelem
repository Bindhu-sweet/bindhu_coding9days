#include 
#define MAX_SIZE 50
int main()
{
int a[MAX_SIZE], num[MAX_SIZE];
int s, i, j, c;
printf("Enter length of array: ");
scanf("%d", &s);
printf("Enter elements of the array array: ");
for(i=0; i<s; i++)
{
scanf("%d", &a[i]);
num[i] = -1;
}

for(i=0; i<s; i++)
{
c = 1;
for(j=i+1; j<s; j++)
{
if(a[i] == a[j])
{
c++;
num[j] = 0;
}
}

if(num[i] != 0)
{
num[i] = c;
}
}

printf("\nUnique elements in the array are: ");
for(i=0; i<s; i++)
{
if(num[i] == 1)
{
printf("%d ", a[i]);
}
}

return 0;
}Â 
